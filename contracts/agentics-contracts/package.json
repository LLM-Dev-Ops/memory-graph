{
  "name": "@agentics/contracts",
  "version": "1.0.0",
  "description": "Schema definitions and type contracts for LLM-Memory-Graph agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./schemas/*": "./schemas/*"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "validate": "ajv validate -s schemas/conversation-memory.schema.json"
  },
  "keywords": [
    "agentics",
    "contracts",
    "llm-memory-graph",
    "schemas",
    "types"
  ],
  "author": "LLM-Dev-Ops",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0",
    "ajv": "^8.12.0",
    "ajv-cli": "^5.0.0"
  },
  "files": [
    "dist",
    "schemas",
    "registry"
  ]
}
