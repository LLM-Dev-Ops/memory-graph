{
  "name": "@llm-memory-graph/knowledge-graph-builder-agent",
  "version": "1.0.0",
  "description": "Knowledge Graph Builder Agent - MEMORY_ANALYSIS agent for building knowledge graphs from text content",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-esm src/index.ts",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "deploy": "gcloud functions deploy knowledge-graph-builder-agent --runtime nodejs20 --trigger-http --entry-point handler"
  },
  "keywords": [
    "llm-memory-graph",
    "agentics",
    "knowledge-graph",
    "memory-analysis",
    "edge-function",
    "entity-extraction",
    "concept-extraction",
    "relationship-inference"
  ],
  "author": "LLM-Dev-Ops",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./extractor": {
      "import": "./dist/extractor.js",
      "types": "./dist/extractor.d.ts"
    },
    "./graph-builder": {
      "import": "./dist/graph-builder.js",
      "types": "./dist/graph-builder.d.ts"
    },
    "./confidence": {
      "import": "./dist/confidence.js",
      "types": "./dist/confidence.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  }
}
