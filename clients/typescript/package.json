{
  "name": "@llm-dev-ops/llm-memory-graph-client",
  "version": "0.1.0",
  "description": "TypeScript/JavaScript client for LLM-Memory-Graph gRPC service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "proto",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --watch",
    "validate": "npm run typecheck && npm run lint && npm run format:check",
    "prepare": "npm run build",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:clean": "rm -rf ../../docs/typescript"
  },
  "keywords": [
    "llm",
    "memory-graph",
    "grpc",
    "typescript",
    "context-tracking",
    "prompt-lineage",
    "graph-database"
  ],
  "author": "LLM DevOps Contributors",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/globalbusinessadvisors/llm-memory-graph.git",
    "directory": "clients/typescript"
  },
  "homepage": "https://github.com/globalbusinessadvisors/llm-memory-graph#readme",
  "bugs": {
    "url": "https://github.com/globalbusinessadvisors/llm-memory-graph/issues"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.5",
    "typedoc": "^0.28.15",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
