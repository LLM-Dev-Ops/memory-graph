{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "out": "../../docs/typescript",
  "name": "LLM Memory Graph TypeScript Client",
  "readme": "README.md",
  "plugin": [],
  "theme": "default",
  "includeVersion": true,
  "excludePrivate": false,
  "excludeProtected": false,
  "excludeExternals": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Client",
    "Types",
    "Errors",
    "Utilities",
    "Validators",
    "Retry",
    "*"
  ],
  "sort": ["source-order"],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": true,
    "external": false
  },
  "commentStyle": "jsdoc",
  "validation": {
    "notExported": true,
    "invalidLink": true,
    "notDocumented": true
  }
}
